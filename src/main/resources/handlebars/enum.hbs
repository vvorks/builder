public enum {{enumName}} {

	{{#each enumValues}}
	{{valueId}}({{code}}){{#separator}},{{/separator}}
	{{/each}}
	;

	private final int code;

	private {{enumName}}(int code) {
		this.code = code;
	}

	public int code() {
		return this.code;
	}

	public static {{enumName}} valueOf(int value) {
		for ({{enumName}} e : values()) {
			if (e.code() == value) {
				return e;
			}
		}
		return null;
	}

}
